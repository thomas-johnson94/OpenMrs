{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/home-quotes.html",
    "src/register-login.html",
    "src/provider-schedules.html",
    "src/secret-quotes.html",
    "src/not-found.html",
    "src/openmrs-home.html",
    "src/appointment-scheduling.html",
    "src/configure-metadata.html",
    "src/patient-register.html",
    "src/man-concepts.html",
    "src/service-types.html",
    "src/man-appointments.html",
    "src/data-management.html",
    "src/active-visits.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
